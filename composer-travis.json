{
    "name": "bean-component/php-geolocation",
    "type": "php-component",
    "description": "Provides a Component for Geolocation Domain Model.",
    "keywords": [ "php geolocation components" ],
    "license": "MIT",
    "authors": [
        {
            "name": "Peter Bean",
            "email": "peterbean@beancomponent.com"
        }
    ],
    "require": {
        "php": "^7.2",
        "bean-component/php-thing": "^1.0.5"
    },
    "require-dev": {
        "php": "^7.2.5",
        "ext-ctype": "*",
        "ext-iconv": "*",
        "symfony/console": "5.0.*",
        "symfony/dotenv": "5.0.*",
        "symfony/flex": "^1.3.1",
        "symfony/framework-bundle": "5.0.*",
        "symfony/yaml": "5.0.*",
        "symfony/test-pack": "^1.0",
        "symfony/orm-pack": "^1.0",
        "symfony/property-access": "^5.0",
        "dama/doctrine-test-bundle": "^6.2",
        "beberlei/doctrineextensions": "^1.2"
    },
    "suggest": {

    },
    "autoload": {
        "psr-4": {
            "Bean\\Geolocation\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "App\\": "src-symfony/",
            "Bean\\Thing\\Doctrine\\": "vendor/bean-component/php-thing/doctrine/",
            "Bean\\Geolocation\\Doctrine\\": "doctrine/",
            "Bean\\Tests\\Geolocation\\": "tests/"
        }
    },
    "replace": {
        "paragonie/random_compat": "2.*",
        "symfony/polyfill-ctype": "*",
        "symfony/polyfill-iconv": "*",
        "symfony/polyfill-php72": "*",
        "symfony/polyfill-php71": "*",
        "symfony/polyfill-php70": "*",
        "symfony/polyfill-php56": "*"
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.x-dev"
        }
    },
    "scripts": {
        "auto-scripts": {
            "cache:clear": "symfony-cmd",
            "assets:install %PUBLIC_DIR%": "symfony-cmd"
        }
    }
}

{
    "name": "bean-component/php-organisation",
    "type": "php-component",
    "description": "Provides a Component for Organisation Domain Model.",
    "keywords": [ "php organisation components" ],
    "license": "MIT",
    "authors": [
        {
            "name": "Peter Bean",
            "email": "peterbean@beancomponent.com"
        }
    ],
    "require": {
        "php": "^7.2",
        "bean-component/php-thing": "^1.0.6",
        "doctrine/collections": "^1.6"
    },
    "require-dev": {
        "php": "^7.2.5",        
        "ext-ctype": "*",
        "ext-iconv": "*",
        "symfony/console": "5.0.*",
        "symfony/dotenv": "5.0.*",
        "symfony/flex": "^1.3.1",
        "symfony/framework-bundle": "5.0.*",
        "symfony/yaml": "5.0.*",
        "symfony/test-pack": "^1.0",
        "symfony/orm-pack": "^1.0",
        "symfony/property-access": "^5.0",
        "dama/doctrine-test-bundle": "^6.2",
        "beberlei/doctrineextensions": "^1.2",
        "jsor/doctrine-postgis": "^1.7",
        "symfony/twig-pack": "^1.0",
        "symfony/maker-bundle": "^1.14",
        "symfony/form": "^5.0",
        "symfony/asset": "^5.0"        
    },
    "suggest": {

    },
    "autoload": {
        "psr-4": {
            "Bean\\Organisation\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "App\\": "src-symfony/",
            "Bean\\Thing\\Doctrine\\": "vendor/bean-component/php-thing/doctrine/",
            "Bean\\Organisation\\Doctrine\\": "doctrine/",
            "Bean\\Tests\\Organisation\\": "tests/"
        }
    },
    "replace": {
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.x-dev"
        }
    },
    "scripts": {
        "auto-scripts": {
            "cache:clear": "symfony-cmd",
            "assets:install %PUBLIC_DIR%": "symfony-cmd"
        }
    }
}
